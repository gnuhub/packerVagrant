<PROCESSDEFINITION WORKFLOWID="LP" AUTHOR="Corey Williams" COUNTRY_KEY="US" CREATED="10-JUNE-2003" NAME="multipleAccountChangePasswordProcessName" DESCRIPTION="multipleAccountChangePasswordProcessDesc" DURATION_UNIT="m" LIMIT="7200000" STATUS="RELEASED" VALID_FROM="10-JUNE-2003" VALID_TO="10-JUNE-2004" VERSION="1.0">
