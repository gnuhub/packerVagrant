  <ACTIVITY ACTIVITYID="ORDEREDPROVISIONLOOP" LIMIT="0" PROCESS="SYNC">
    <IMPLEMENTATION_TYPE>
      <LOOP TYPE="WHILE">
        <SCRIPT>
          var previousActivityResult = &quot;&quot;;
          var previousActivity = null;
          if (loopcount &gt; 1) {
            previousActivity = process.getActivity(&quot;ORDEREDPROVISIONSUB&quot;, loopcount-1);
            if (previousActivity != null) {
              previousActivityResult = previousActivity.resultSummary;
            }
          }
          var provisioningRequests = WorkflowRuntimeContext.getDataValue(&quot;provisioningRequests&quot;);
          var size = provisioningRequests.size();
          if(previousActivityResult != &quot;&quot; &amp;&amp; previousActivityResult != &quot;SS&quot; &amp;&amp; previousActivityResult != &quot;SW&quot; ) {
            if(loopcount &lt;= size ) {
              var arrFailedServiceDN = new Array(size);
              arrFailedServiceDN[0] = provisioningRequests.get(loopcount-2).getService().getDistinguishedName().getAsString().toLowerCase();
              var failedServiceIndex = 1;
              for(i = loopcount - 1; i &lt; size ; i++) {
                var provisioningRequest = provisioningRequests.get(i);
                var service = provisioningRequest.getService();
                var pendingServiceDN = service.getDistinguishedName().getAsString().toLowerCase();
                var prerequisites = service.getPrerequisiteDNs();
                if(prerequisites != null &amp;&amp; !prerequisites.isEmpty()) {
                  var arrPrerequisites = prerequisites.toArray();
                  var numPrerequisites = arrPrerequisites.length;
                  var removeResult = false;
                  for(j = 0; j &lt; numPrerequisites; j++) {
                    var dn = arrPrerequisites[j].getAsString().toLowerCase();
                    for(k = 0; k &lt; failedServiceIndex; k ++) {
                      if(dn == arrFailedServiceDN[k]) {
                        removeResult = provisioningRequests.remove(provisioningRequest);
                        if(removeResult) {
                          i--;
                          size --;
                        }
                        arrFailedServiceDN[failedServiceIndex ++] = pendingServiceDN;
                        k = failedServiceIndex;
                        j = numPrerequisites + 1;
                      }
                    }
                  }
                }
              }
              if(size &gt; 0 &amp;&amp; failedServiceIndex &gt; 1) {
                WorkflowRuntimeContext.setDataValue(&quot;provisioningRequests&quot;, provisioningRequests);
              }
            }
          }
          return (loopcount &lt;= size);
        </SCRIPT>
      </LOOP>
    </IMPLEMENTATION_TYPE>
  </ACTIVITY>
  <ACTIVITY ACTIVITYID="ORDEREDPROVISIONSUB" LIMIT="3600000">
