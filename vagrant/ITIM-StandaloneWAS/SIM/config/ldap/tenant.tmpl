        <TRANSITION_KIND>
            <REGULAR FROM="LoopOverRolesOfPersonId" TO="END">
                <SCRIPT>true</SCRIPT>
            </REGULAR>
        </TRANSITION_KIND>
    </TRANSITION>
    <TRANSITION TRANSITIONID="id2010774281296329548">
        <TRANSITION_KIND>
            <REGULAR FROM="START" TO="LoopOverRolesOfPersonId">
                <SCRIPT><![CDATA[
var p = person.get();
var rl = rolelist.get();
if (rl == null) {
    var newRoles = p.getNewRoles();
    rolelist.set(newRoles);
}
return true;]]>]]&gt;<![CDATA[</SCRIPT>
            </REGULAR>
        </TRANSITION_KIND>
    </TRANSITION>
    <TRANSITION TRANSITIONID="id2645657261905554310">
        <TRANSITION_KIND>
            <LOOP_BEGIN FROM_LOOP="LoopOverRolesOfPersonId" TO_LOOP="CallApprovalOperationId"/>
        </TRANSITION_KIND>
    </TRANSITION>
    <TRANSITION TRANSITIONID="id2645667072114087749">
        <TRANSITION_KIND>
            <LOOP_END FROM_LOOP="CallApprovalOperationId" TO_LOOP="LoopOverRolesOfPersonId"/>
        </TRANSITION_KIND>
    </TRANSITION>
