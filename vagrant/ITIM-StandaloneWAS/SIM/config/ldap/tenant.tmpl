      <attr name="erXML">
        <value><![CDATA[<?xml version="1.0" encoding="UTF-8"?>
<PROCESSDEFINITION CLASSIFICATION="SO" COUNTRY_KEY="US" LIMIT="0"     NAME="processSoDViolations" SCHEMA_VERSION="1.0" STATUS="UNDER_TEST"     VERSION="1.0" WORKFLOWID="DE">
    <PARAMETERS>
        <IN_PARAMETERS TYPE="Person" EXTENDED_ATTRIBUTE="person" PARAM_ID="person" RELEVANT_DATA_ID="person" />
        <IN_PARAMETERS TYPE="OrganizationalContainer" EXTENDED_ATTRIBUTE="container" PARAM_ID="container" RELEVANT_DATA_ID="container" />
        <IN_PARAMETERS TYPE="List" COLLECTION_TYPE="SeparationOfDutyRuleViolation" EXTENDED_ATTRIBUTE="violations" PARAM_ID="violations" RELEVANT_DATA_ID="violations" />
        <IN_PARAMETERS TYPE="String" EXTENDED_ATTRIBUTE="suspendAccounts" PARAM_ID="suspendAccounts" RELEVANT_DATA_ID="suspendAccounts" />
        <IN_PARAMETERS TYPE="String" EXTENDED_ATTRIBUTE="extensionToCall" PARAM_ID="extensionToCall" RELEVANT_DATA_ID="extensionToCall" />
        <OUT_PARAMETERS TYPE="Person" EXTENDED_ATTRIBUTE="person" PARAM_ID="person" RELEVANT_DATA_ID="person" />
    </PARAMETERS>
    <ACTIVITY ACTIVITYID="START" COMPLETE_ON_TIMEOUT="false" LIMIT="0" NAME="Start Activity" PRIORITY="NORMAL" SKIP_ESCALATION="false">
        <IMPLEMENTATION_TYPE>
            <ROUTE/>
        </IMPLEMENTATION_TYPE>
        <TRANSITION_RESTRICTION SPLIT="AND"/>
    </ACTIVITY>
    <ACTIVITY ACTIVITYID="END" COMPLETE_ON_TIMEOUT="false" LIMIT="0" NAME="End Activity" PRIORITY="NORMAL" SKIP_ESCALATION="false">
        <IMPLEMENTATION_TYPE>
            <ROUTE/>
        </IMPLEMENTATION_TYPE>
        <TRANSITION_RESTRICTION JOIN="XOR"/>
    </ACTIVITY>
    <ACTIVITY ACTIVITYID="approvalLoop" COMPLETE_ON_TIMEOUT="false" LIMIT="0" NAME="SoD Violation Approval Loop" PRIORITY="NORMAL" PROCESS="ASYNC" SKIP_ESCALATION="false">
        <IMPLEMENTATION_TYPE>
            <LOOP TYPE="WHILE">
                <SCRIPT>
var bl = blockAudit.get();
return ((loopcount&lt;=WorkflowRuntimeContext.getCollectionDataValue("violations").size()) &amp;&amp; ("REJECTED" != bl));
                </SCRIPT>
            </LOOP>
        </IMPLEMENTATION_TYPE>
        <TRANSITION_RESTRICTION JOIN="AND" SPLIT="AND"/>
    </ACTIVITY>
    <ACTIVITY ACTIVITYID="createPerson" COMPLETE_ON_TIMEOUT="false" LIMIT="0" PRIORITY="NORMAL" SKIP_ESCALATION="false">
        <IMPLEMENTATION_TYPE>
            <APPLICATION CLASS_NAME="com.ibm.itim.workflowextensions.PersonExtensions" METHOD_NAME="createPerson" TOOLNAME="SYSTEM"/>
        </IMPLEMENTATION_TYPE>
        <TRANSITION_RESTRICTION JOIN="AND" SPLIT="AND"/>
        <PARAMETERS>
