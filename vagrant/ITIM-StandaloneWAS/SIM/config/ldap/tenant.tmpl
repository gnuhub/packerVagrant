	<PARAMETERS>
		<IN_PARAMETERS
		  PARAM_ID="accounts"
		  TYPE="List"
		  COLLECTION_TYPE="Account"
		  RELEVANT_DATA_ID="accounts"
		/>
		<IN_PARAMETERS
		  PARAM_ID="notifyFlag"
		  TYPE="String"
		  RELEVANT_DATA_ID="notifyFlag"
		/>
	</PARAMETERS>

	<!-- Process Start -->
	<ACTIVITY ACTIVITYID="START" LIMIT="0">
		<IMPLEMENTATION_TYPE>
			<ROUTE/>
		</IMPLEMENTATION_TYPE>
      <SCRIPT EVENT="onComplete">
var sameOwner = "true";
var accountsCollection = WorkflowRuntimeContext.getCollectionDataValue("accounts");
var accountsSize = accountsCollection.size();

var firstAccount = accountsCollection.get(0);
var firstAccountOwner = firstAccount.getOwnerDN();

for (i=1;i&lt;accountsSize;i++) {
    var acct = accountsCollection.get(i);
    var ownerDN = acct.getOwnerDN();
    if (ownerDN.toString() != firstAccountOwner.toString()) {
        sameOwner = "false";
        break;
    }
}
if (sameOwner == "true") {
    var accountOwner = new Person(firstAccountOwner.toString());
    process.setRequesteeData(accountOwner);
}
      </SCRIPT>
	</ACTIVITY>

