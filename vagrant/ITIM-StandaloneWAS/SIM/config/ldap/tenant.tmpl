        <IN_PARAMETERS ATTRIBUTE_DESCRIPTION="Role list of the person."             COLLECTION_TYPE="Role" EXTENDED_ATTRIBUTE="rolelist"             PARAM_ID="rolelist" RELEVANT_DATA_ID="rolelist" TYPE="List"/>
        <IN_PARAMETERS ATTRIBUTE_DESCRIPTION="The default approval operation name."            EXTENDED_ATTRIBUTE="approvalOperationName"             PARAM_ID="approvalOperationName" RELEVANT_DATA_ID="approvalOperationName" TYPE="String"/>
        <OUT_PARAMETERS             ATTRIBUTE_DESCRIPTION="A person with approved roles by the role owner."             EXTENDED_ATTRIBUTE="person" PARAM_ID="person"             RELEVANT_DATA_ID="person" TYPE="Person"/>
    </PARAMETERS>
    <ACTIVITY ACTIVITYID="START" COMPLETE_ON_TIMEOUT="false" LIMIT="0"         NAME="Start Activity" PRIORITY="NORMAL" SKIP_ESCALATION="false">
        <IMPLEMENTATION_TYPE>
            <ROUTE/>
        </IMPLEMENTATION_TYPE>
        <TRANSITION_RESTRICTION SPLIT="AND"/>
    </ACTIVITY>
    <ACTIVITY ACTIVITYID="END" COMPLETE_ON_TIMEOUT="false" LIMIT="0"         NAME="End Activity" PRIORITY="NORMAL" SKIP_ESCALATION="false">
        <IMPLEMENTATION_TYPE>
            <ROUTE/>
        </IMPLEMENTATION_TYPE>
        <TRANSITION_RESTRICTION JOIN="XOR"/>
    </ACTIVITY>
    <ACTIVITY ACTIVITYID="LoopOverRolesOfPersonId"         COMPLETE_ON_TIMEOUT="false"         DESCRIPTION="Loop over the new roles of a person" LIMIT="0"         NAME="LoopOverRolesOfPersonName" PRIORITY="NORMAL"         PROCESS="ASYNC" SKIP_ESCALATION="false">
        <IMPLEMENTATION_TYPE>
            <LOOP TYPE="WHILE">
                <SCRIPT><![CDATA[loopcount<=WorkflowRuntimeContext.getCollectionDataValue("rolelist").size()]]>]]&gt;<![CDATA[</SCRIPT>
            </LOOP>
        </IMPLEMENTATION_TYPE>
        <TRANSITION_RESTRICTION JOIN="AND" SPLIT="AND"/>
    </ACTIVITY>
    <ACTIVITY ACTIVITYID="CallApprovalOperationId"         COMPLETE_ON_TIMEOUT="false"         DESCRIPTION="Call the default approveRole operation." LIMIT="0"         NAME="CallApprovalOperationName" PRIORITY="NORMAL" SKIP_ESCALATION="false">
        <IMPLEMENTATION_TYPE>
            <APPLICATION                 CLASS_NAME="com.ibm.itim.workflowextensions.RoleExtensions"                 METHOD_NAME="callApprovalOperation" TOOLNAME="SYSTEM"/>
        </IMPLEMENTATION_TYPE>
        <TRANSITION_RESTRICTION JOIN="AND" SPLIT="AND"/>
        <PARAMETERS>
            <IN_PARAMETERS EXTENDED_ATTRIBUTE="person" PARAM_ID="person"                 RELEVANT_DATA_ID="person" TYPE="Person"/>
            <IN_PARAMETERS EXTENDED_ATTRIBUTE="role" PARAM_ID="role"                  TYPE="OrgRole" EXPRESSION="rolelist.get()[loopcount-1]"/>
            <IN_PARAMETERS EXTENDED_ATTRIBUTE="approvalOperationName"                 PARAM_ID="approvalOperationName"                 RELEVANT_DATA_ID="approvalOperationName" TYPE="String"/>
        </PARAMETERS>
        <SCRIPT EVENT="onComplete"><![CDATA[if (activity.resultSummary==activity.REJECTED) {
    var rejectedRole = rolelist.get()[loopcount-1];
    var p = person.get();
    p.removeRole(rejectedRole);
    person.set(p);
}
WorkflowRuntimeContext.setProcessResult(WorkflowRuntimeContext.getActivityResult());
WorkflowRuntimeContext.setProcessResultDetail(WorkflowRuntimeContext.getActivityResultDetail());
]]>]]&gt;<![CDATA[</SCRIPT>
    </ACTIVITY>
