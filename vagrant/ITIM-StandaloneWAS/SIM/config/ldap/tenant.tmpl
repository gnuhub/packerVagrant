    <!-- ======================================================== -->
    <!--                   Operation global.ordereddeprovision    -->
    <!-- ======================================================== -->
    <entry dn="erglobalid=<%operation.global.ordereddeprovision%>,ou=operations,ou=itim,<%tenant.dn%>">
      <objectclass>
        <oc-value>top</oc-value>
        <oc-value>erManagedItem</oc-value>
        <oc-value>erWorkflowDefinition</oc-value>
      </objectclass>
      <attr name="erglobalid"><value><%operation.global.ordereddeprovision%></value></attr>
      <attr name="erversionid"><value><%operation.global.ordereddeprovision%></value></attr>
      <attr name="erCategory"><value>Global</value></attr>
      <attr name="erProcessName"><value>ordereddeprovision</value></attr>
      <attr name="erXML">
      	<value><![CDATA[<?xml version="1.0" encoding="UTF-8"?>
      		<PROCESSDEFINITION NAME="globalDOAProcessName" WORKFLOWID="DO" AUTHOR="Corey Williams" COUNTRY_KEY="US" DESCRIPTION="globalDOAProcessDesc" DURATION_UNIT="m" LIMIT="43200000" CREATED="04-March-2003" VALID_FROM="04-March-2003" VALID_TO="04-March-2004" CLASSIFICATION="SO" VERSION="1.0">
			<ESCALATION_PARTICIPANT PARTICIPANTID="SystemAdmin" TYPE="SA" NAME=""/>
			<PARAMETERS>
					<IN_PARAMETERS PARAM_ID="ACCOUNTLIST" TYPE="List" COLLECTION_TYPE="Account" RELEVANT_DATA_ID="ACCOUNTLIST"/>
			</PARAMETERS>
			<!-- Start -->
			<ACTIVITY ACTIVITYID="START" LIMIT="0">
				<IMPLEMENTATION_TYPE>
					<ROUTE/>
				</IMPLEMENTATION_TYPE>
			</ACTIVITY>
			<!-- Ordered DEProvisioning Loop -->
			<ACTIVITY ACTIVITYID="ORDEREDDEPROVISIONLOOP" LIMIT="0" PROCESS="SYNC">
				<IMPLEMENTATION_TYPE>
					<LOOP TYPE="WHILE" CONDITION='loopcount&lt;=WorkflowRuntimeContext.getCollectionDataValue("ACCOUNTLIST").size()'/>
				</IMPLEMENTATION_TYPE>
			</ACTIVITY>
			<ACTIVITY ACTIVITYID="ORDEREDDEPROVISIONSUB" LIMIT="3600000">
				<IMPLEMENTATION_TYPE>
					<OPERATION OPERATION_NAME="delete">
						<ENTITY_OPERATION ENTITY_EXP='WorkflowRuntimeContext.getDataValue("ACCOUNTLIST").get(loopcount-1)'/>
					</OPERATION>
				</IMPLEMENTATION_TYPE>
				</ACTIVITY>
			<!-- END -->
			<ACTIVITY ACTIVITYID="END" LIMIT="0">
				<IMPLEMENTATION_TYPE>
					<ROUTE/>
				</IMPLEMENTATION_TYPE>
			</ACTIVITY>
			<!-- Transistions -->
			<!-- START -->
			<TRANSITION TRANSITIONID="GOTO_ORDEREDDEPROVISION">
				<TRANSITION_KIND>
					<REGULAR FROM="START" TO="ORDEREDDEPROVISIONLOOP"/>
				</TRANSITION_KIND>
			</TRANSITION>
			<!-- Do ordered provisioning -->
			<TRANSITION TRANSITIONID="ORDEREDDEPROVISIONLOOPBEGIN">
				<TRANSITION_KIND>
					<LOOP_BEGIN TO_LOOP="ORDEREDDEPROVISIONSUB" FROM_LOOP="ORDEREDDEPROVISIONLOOP"/>
				</TRANSITION_KIND>
			</TRANSITION>
			<TRANSITION TRANSITIONID="ORDEREDDEPROVISIONLOOPEND">
				<TRANSITION_KIND>
					<LOOP_END FROM_LOOP="ORDEREDDEPROVISIONSUB" TO_LOOP="ORDEREDDEPROVISIONLOOP"/>
				</TRANSITION_KIND>
			</TRANSITION>
			<TRANSITION TRANSITIONID="COMPLETE">
				<TRANSITION_KIND>
					<REGULAR FROM="ORDEREDDEPROVISIONLOOP" TO="END"/>
				</TRANSITION_KIND>
			</TRANSITION>
			<!--RELEVANT_DATA RELEVANT_DATA_ID="deprovisioningRequests" TYPE="List" COLLECTION_TYPE="Account" NAME="deprovisioningRequests" DESCRIPTION="Ordered deprovisioning requests."/-->
			<RELEVANT_DATA COLLECTION_TYPE="Account"
				DESCRIPTION="User account list" NAME="ACCOUNTLIST"
			RELEVANT_DATA_ID="ACCOUNTLIST" TYPE="List"/>
		</PROCESSDEFINITION>]]>
	    </value>
      </attr>
    </entry>

